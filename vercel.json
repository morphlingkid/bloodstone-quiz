{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/auth",
        "dest": "/server.js"
      },
      {
        "src": "/save-result",
        "dest": "/server.js"
      },
      {
        "src": "/oauth2callback",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }